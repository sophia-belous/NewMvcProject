@using NewBlog.Domain.Entities
@model IList<Post>
<li class="nav-header"><h2>Latest Posts</h2></li>

        @if (Model.Count > 0)
        {
            foreach (var post in Model)
            {
                <li>@Html.PostLink(post)</li>
            }
        }
        else
        {
            <p>No posts found!</p>
        }


@using NewBlog.WebUI.Models
@using NewBlog.Domain.Entities;
@model ListViewModel

<div id="content">
    <center><h1>@ViewBag.Title</h1></center>

    @Html.Partial("_Pager", Model)

    @foreach (var post in Model.Posts)
    {
        <div class="jumbotron">
            @Html.Partial("_PostTemplate", post)
        </div>
    }
</div>

